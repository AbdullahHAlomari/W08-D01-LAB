generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model classroom {
  id   String @id @default(uuid())
  name String
}

model student {
  id    String @id @default(uuid())
  name  String
  major String
  age   Int
}

model teacher {
  id   String @id @default(uuid())
  name String
}
